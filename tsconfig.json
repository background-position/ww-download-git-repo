{
  "compilerOptions": {
    /* Basic Options */
    // "incremental": true,                   /* Enable incremental compilation */
    "target": "ESNext",                        
    "baseUrl": "./",
    "paths": {
        "@/*": ["src/*"],
        "@@/*": ["src/.umi/*"]
      },
    "esModuleInterop": true,
    "outDir": "./dist/index.js",   
    "declaration":true,   
    "forceConsistentCasingInFileNames": true ,
    "moduleResolution": "Node" ,
  }
}
